<int factorial(int a)/>
    <if a == 0 || a == 1/>
        <ret 1/>
    </if>
    <ret a * factorial(a-1)/>
</factorial>


<int a = factorial(10)/>
<print(a)/>

<int sumDown(int n, int acc)/>
    <if n > 0/>
        <ret sumDown(n - 1, acc + n)/>
    </if>
    <ret acc/>
</sumDown>

<int result = sumDown(4, 0)/>
<print(result)/>